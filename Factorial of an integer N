public class Main {

    public static void printFactorial(int n, int fact) {
        if (n == 0) {
            System.out.println(fact);
            return;
        }
        fact *= n;
        printFactorial(n-1, fact);
    }

    public static void main(String[] args) {
        int fact = 1;
        int n = 5;
        printFactorial(n,fact);

    }
}
